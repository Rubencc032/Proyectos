<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="439.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
		
   <children>
      <Label fx:id="username" layoutX="100.0" layoutY="160.0" prefHeight="21.0" prefWidth="91.0" text="UserName">
         <font>
            <Font name="Comic Sans MS Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="100.0" layoutY="230.0" prefHeight="20.0" prefWidth="71.0" text="Password">
         <font>
            <Font name="Comic Sans MS Bold" size="12.0" />
         </font>
      </Label>
      <TextField layoutX="190.0" layoutY="160.0" prefHeight="20.0" prefWidth="161.0" />
      <PasswordField layoutX="190.0" layoutY="228.0" prefHeight="25.0" prefWidth="161.0" />
      <ComboBox fx:id="mycombobox" layoutX="150.0" layoutY="90.0" prefHeight="22.0" prefWidth="131.0" visibleRowCount="3" />
      <Button layoutX="170.0" layoutY="310.0" mnemonicParsing="false" onAction="handleSubmitButtonAction()" prefHeight="31.0" prefWidth="101.0" text="Login">
         <font>
            <Font name="Comic Sans MS Bold" size="12.0" />
         </font>
         <fx:script>
         	function handleSubmitButtonAction() {
         		username.setText("Calling the Javascript");
         	}
         </fx:script>
      </Button>
   </children>
</AnchorPane>
