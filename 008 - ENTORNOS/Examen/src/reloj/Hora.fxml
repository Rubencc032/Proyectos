<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="369.0" prefWidth="281.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="reloj.HoraController">
   <children>
      <Label layoutX="102.0" layoutY="40.0" text="HORA">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="hora" layoutX="121.0" layoutY="109.0" text="12">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ComboBox fx:id="cbFormato" layoutX="60.0" layoutY="160.0" onAction="#cambiarFormato" prefWidth="150.0" />
      <Button fx:id="btnVolver" layoutX="109.0" layoutY="246.0" mnemonicParsing="false" onAction="#volver" text="Volver">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="incremento" layoutX="210.0" layoutY="117.0" mnemonicParsing="false" onAction="#incrementarHora" prefHeight="35.0" prefWidth="52.0" text="+" />
      <Button fx:id="decremento" layoutX="210.0" layoutY="203.0" mnemonicParsing="false" onAction="#decrementarHora" prefHeight="35.0" prefWidth="52.0" text="-" />
   </children>
</AnchorPane>
