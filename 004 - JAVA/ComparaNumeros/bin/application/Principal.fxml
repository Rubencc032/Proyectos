<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="400.0" style="-fx-background-color: AliceBlue;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PrincipalController">
   <top>
      <Label text="Compara Numeros" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets top="20.0" />
         </BorderPane.margin>
         <font>
            <Font name="Agency FB" size="24.0" />
         </font>
      </Label>
   </top>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="87.0" layoutY="78.0" text="Numero A">
               <font>
                  <Font name="Agency FB" size="18.0" />
               </font>
            </Label>
            <Label layoutX="87.0" layoutY="137.0" text="Numero B">
               <font>
                  <Font name="Agency FB" size="18.0" />
               </font>
            </Label>
            <Label fx:id="lbMensajes" alignment="CENTER" contentDisplay="CENTER" layoutX="32.0" layoutY="277.0" prefHeight="20.0" prefWidth="340.0" text="Introduzca 2 numeros enteros" textAlignment="CENTER" textFill="#ee1111">
               <font>
                  <Font name="Agency FB Bold" size="14.0" />
               </font>
            </Label>
            <Button fx:id="btnComparar" layoutX="158.0" layoutY="202.0" mnemonicParsing="false" onAction="#compararNumeros" prefHeight="38.0" prefWidth="85.0" text="Comparar">
               <font>
                  <Font name="Agency FB" size="14.0" />
               </font>
            </Button>
            <TextField fx:id="tfNum1" layoutX="168.0" layoutY="74.0" prefHeight="33.0" prefWidth="151.0" />
            <TextField fx:id="tfNum2" layoutX="168.0" layoutY="133.0" prefHeight="32.0" prefWidth="153.0" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
